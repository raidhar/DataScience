  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "Q1_cred = credit_df.quantile(0.25)\n",
    "Q3_cred = credit_df.quantile(0.75)\n",
    "IQR = Q3_cred - Q1_cred"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "(4679058, 43)"
      ]
     },
     "execution_count": 13,
